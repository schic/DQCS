<configuration xmlns="http://eobjects.org/analyzerbeans/configuration/1.0">
  <reference-data-catalog>
    <synonym-catalogs>
      <text-file-synonym-catalog name="textfile sc">
        <filename>/foo/bar.txt</filename>
        <encoding>UTF8</encoding>
        <case-sensitive>false</case-sensitive>
      </text-file-synonym-catalog>
      <datastore-synonym-catalog name="ds sc">
        <datastore-name>orderdb</datastore-name>
        <master-term-column-path>products.productname</master-term-column-path>
        <synonym-column-path>products.productline</synonym-column-path>
        <synonym-column-path>product.producttype</synonym-column-path>
        <load-into-memory>false</load-into-memory>
      </datastore-synonym-catalog>
    </synonym-catalogs>
  </reference-data-catalog>
</configuration>
